.ORIG 0x3000

LABEL ADD R0, R1, R2 ; R0 = R1 + R2
LABEL ADD R0, R1, #3; R0 = R1 + R2
LD R3, LABEL
LDI R2, HERE
LDR R1, R2, #5
STR R1, R2, #5
RTI

AND R7, R2, R1
AND R0, R7, #7

NOT R1, R2

LEA R2, #3000

ST R2, #3000
STI R2, LABEL

BR  LABEL
BRn LABEL
BRzp LABEL
BRz LABEL
BRnp LABEL
BRp LABEL
BRnz LABEL
BRnzp LABEL

; I hope it doesn't produce infinite loop:)
JSR HERE

HERE LD R1, EXIT
LD R2, #3000
JSRR R2

TOJUMP JMP R1

EXIT RET

TRAP 0x31

.END